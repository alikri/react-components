{
  "name": "react-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --ci --runInBand --coverage --reporters='default' --reporters='jest-junit' --watchAll=false",
    "format:fix": "prettier --config .prettierrc.cjs \"**/*.{ts,tsx,css}\" --write",
    "format": "prettier --config .prettierrc.cjs \"**/*.{ts,tsx,css}\" --check",
    "prepare": "husky install",
    "pre-commit": "npm run format && npm run lint"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "axios-cache-interceptor": "^1.3.2",
    "jest-environment-jsdom": "^29.7.0",
    "pokenode-ts": "^1.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
